    "openblocks-dev-utils": "workspace:^",
    "react": "^17",
    "react-dom": "^17",
    "react-json-view": "^1.21.3",
    "rollup-plugin-external-globals": "^0.7.1",
    "vite": "^3.2.4",
    "vite-plugin-css-injected-by-js": "^2.1.1",
    "vite-plugin-svgr": "^2.2.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "typescript": "^4.8.4"
